@model flip_flop.Models.TicketsHistory

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<h4>TicketsHistory</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Key" />
