                <label asp-for="KeySeller" class="control-label"></label>
                <select asp-for="KeySeller" class="form-control" asp-items="ViewBag.KeySeller"></select>
                <span asp-validation-for="KeySeller" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="KeyBuyer" class="control-label"></label>
                <select asp-for="KeyBuyer" class="form-control" asp-items="ViewBag.KeyBuyer"></select>
                <span asp-validation-for="KeyBuyer" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="KeyTicket" class="control-label"></label>
                <select asp-for="KeyTicket" class="form-control" asp-items="ViewBag.KeyTicket"></select>
                <span asp-validation-for="KeyTicket" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DateOfTrade" class="control-label"></label>
                <input asp-for="DateOfTrade" class="form-control" />
                <span asp-validation-for="DateOfTrade" class="text-danger"></span>
